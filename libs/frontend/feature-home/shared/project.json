    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/frontend/feature-home/shared/**/*.{ts,tsx,js,jsx}"
        ]
      }
